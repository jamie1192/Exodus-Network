<?xml version="1.0" encoding="UTF-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <item>

        <shape>

            <solid android:color="@color/warmindDark" />

            <stroke android:width="1dp"
                android:color="@color/placeholderHint" />

            <padding
                android:left="2dp"
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp" />

        </shape>

    </item>

    <item
        android:top="1dp"
        android:left="1dp"
        android:right="1dp"
        android:bottom="1dp"
        >
        <shape>

            <solid android:color="@color/completed_green" />



            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>


</layer-list>
